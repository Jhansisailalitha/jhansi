#include <stdio.h>
#include <string.h>

 
int main () 
{
  
char str[20];
  
int i, n, vowel, consonant;
  
printf ("Enter name: ");
  
gets (str);
  
 
 
vowel = 0;
  
consonant = 0;
  
n = strlen (str);
for (i = 0; i < n; i++)
    
    {
    switch (str[i])
	
	{
	
case 'a':
	
case 'e':
	
case 'i':
	
case 'o':
	
case 'u':
	
case 'A':
	
case 'E':
	
case 'I':
	
case 'O':
	
case 'U':
	  
vowel++;
	  
break;
	
default:
	  consonant++;
	
}
    
}
  printf ("no of vowel = %d\n", vowel);
  
printf ("no of consonant = %d\n", consonant);
  
 
return 0;

}
