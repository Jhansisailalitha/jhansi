#include <stdio.h>
#include<conio.h>
void zerostolast(int a[], int n)
{
int v= 0;
 for(int i = 0; i < n; i++)
{
        
       if (a[i] != 0) {
            a[v++] = a[i];
        }
    }
for (int i = v; i < n; i++) {
        a[i] = 0;
    }
}
 
int main(void)
{
    int a[] = {3,0,0,4,0,6 };
    int n = sizeof(a) / sizeof(a[0]);
 
    (a, n);
 
    for (int i = 0; i < n; i++) {
        printf("%d ", a[i]);
    }
 
    return 0;
}
