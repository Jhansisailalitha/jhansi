#include <stdio.h>

void main()
{
    int array[30],i,n;
    printf("Enter the size");
    scanf("%d",&n);
    printf("Enter the elements ");
    for(i=0; i<n; i++)
    {
        scanf("%d",&array[i]);
    }
    printf("Even ");
     for(i=0; i<n; i++)
     {
        if(array[i]%2==0)
        {
            printf("%d \t",array[i]);
        }
    }
    printf("Odd ");
     for(i=0; i<=n; i++)
     {
        if (array[i]%2==1)
        {
            printf("%d \t",array[i]);
        }
    }
}
